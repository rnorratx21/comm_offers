<script type="text/javascript">
//<![CDATA[s
//Rounded corners
$(function() {
  if (!$.browser.msie) { 
	  $('.rounded').corner();
	}
		
	$('#flash div').each(function() {
		var flashColor = $(this).css('color');
		$(this).effect("highlight", {color: flashColor}, 1000);
	});
	
});
//]]>
</script>
