  'freshfire/css/ie', 'freshfire/css/jquery.fancybox-1.3.1', 'freshfire/css/skin-1', 'jquery-humanity/jquery-ui-1.8rc1.custom', 'community-offers', 'tables', 'footer', 'menu',
